Algoritmo son_Iguales
	Definir A,B Como Real;
    Definir sonIguales Como Logico;
    
    Escribir "Ingrese el primer número:";
    Leer A;
    
    Escribir "Ingrese el segundo número:";
    Leer B;
    
    sonIguales = A = B;
    
    Escribir "¿Los números son iguales?: ", sonIguales;
FinAlgoritmo
